[
	{
		"scopes": ["iw3|sp-mp", "iw4|sp"],
		"usage": "getWeaponClipModel(<weapon>)",
		"description": [
			"Returns the name of the weapon clip/magazine model used for the given weapon."
		],
		"params": [
			{
				"name": "weapon",
				"types": ["string"],
				"description": ["The weapon name."],
				"optional": false
			}
		],
		"return": { "types": ["string"] },
		"example": ["model = getWeaponClipModel(\"m16_basic\");"]
	}
]
