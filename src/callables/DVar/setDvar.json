[
	{
		"scopes": ["iw3|sp-mp"],
		"usage": "setDvar(<dvar>, <value>)",
		"description": [
			"Sets the value of a script dvar. Can not set code dvars. Script dvars are not preserved in savegames."
		],
		"params": [
			{ "name": "dvar", "types": ["string"], "description": ["The dvar name."], "optional": false },
			{
				"name": "value",
				"types": ["string"],
				"description": ["The dvar value."],
				"optional": false
			}
		],
		"example": ["setDvar(\"r_eyesAdjust\", \"1\");"]
	},
	{
		"scopes": ["iw4|sp-mp"],
		"description": ["Set the value of a DVar."],
		"params": [
			{ "name": "dvar", "types": ["string"], "description": ["Name of the DVar."] },
			{
				"name": "value",
				"types": ["string", "int", "float", "vector"],
				"description": ["Value to set."]
			}
		]
	}
]
