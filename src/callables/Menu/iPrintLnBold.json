[
	{
		"scopes": ["iw3|sp-mp"],
		"usage": "iPrintLnBold(<text>)",
		"description": ["Write bold line to the screen."],
		"params": [
			{
				"name": "text",
				"types": ["string"],
				"description": ["Text to be written."],
				"optional": false
			}
		],
		"example": ["iPrintLnBold(\"Mitchell!\");"]
	},
	{
		"scopes": ["iw4|sp-mp"],
		"description": ["Write a line of text to the center output."],
		"params": [{ "name": "text", "types": ["string", "int", "float", "vector", "player"] }],
		"paramsRepeatable": "last",
		"receiver": {
			"types": ["world", "player"],
			"description": ["If called on a player, only they will see the line."]
		}
	}
]
